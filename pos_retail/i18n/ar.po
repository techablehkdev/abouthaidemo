#. module: pos_retail
#: model:mail.template,body_html:pos_retail.email_template_edi_pos_orders
#, fuzzy
msgid ""
"\n"
"                <p>Dear ${object.partner_id.name}\n"
"                    % set access_action = object.with_context(force_website=True).get_access_action()\n"
"                    % set doc_name = 'order' if object.state in ('new','paid', 'done', 'invoiced') else 'order confirmation'\n"
"                    % set is_online = access_action and access_action['type'] == 'ir.actions.act_url'\n"
"                    % set access_name = is_online and object.template_id and 'Accept and pay %s online' % doc_name or 'View %s' % doc_name\n"
"                    % set access_url = is_online and access_action['url'] or None\n"
"\n"
"                    % if object.partner_id.parent_id:\n"
"                        (<i>${object.partner_id.parent_id.name}</i>)\n"
"                    % endif\n"
"                    ,</p>\n"
"                    <p>\n"
"                    Here is your ${doc_name} <strong>${object.name}</strong>\n"
"                    % if object.origin:\n"
"                    (with reference: ${object.origin} )\n"
"                    % endif\n"
"                    amounting in <strong>${object.amount_total} ${object.pricelist_id.currency_id.name}</strong>\n"
"                    from ${object.company_id.name}.\n"
"                    </p>\n"
"                    % if is_online:\n"
"                        <center>\n"
"                            <a href=\"${access_url}\" style=\"background-color: #1abc9c; padding: 20px; text-decoration: none; color: #fff; "
"border-radius: 5px; font-size: 16px;\" class=\"o_default_snippet_text\">${access_name}</a>\n"
"                            <br/><br/>\n"
"                            <span style=\"color:#888888\">(or view attached PDF)</span>\n"
"                        </center>\n"
"                        <br/>\n"
"                    % endif\n"
"\n"
"                    <p>You can reply to this email if you have any questions.</p>\n"
"                    <p>Thank you,</p>\n"
"\n"
"                    <p style=\"color:#eeeeee;\">\n"
"                    % if object.user_id and object.user_id.signature:\n"
"                        ${object.user_id.signature | safe}\n"
"                    % endif\n"
"                    </p>\n"
"                                    \n"
"            "
msgstr ""
"<p style=\";text-align:right;direction:rtl\">عزيزي $ {object.partner_id.name} % s et access_action = object.with_context (force_website = "
"True) .get_access_action () % s et doc_name = 'order' if object.state in ('new'، 'pay'، 'done'، 'invoiced') else 'تأكيد الطلب' % s et "
"is_online = access_action and access_action ['type'] == 'ir.actions.act_url' % s et access_name = is_online and object.template_id و 'Accept "
"and pay {5 } online '% d oc_name أو' View %s '% d oc_name % s et access_url = is_online and access_action [' url '] أو بلا % i f object."
"partner_id.parent_id: ( <i>$ {object.partner_id.parent_id.name}</i> ) % e ndif،</p><p style=\";text-align:right;direction:rtl\"> إليك $ "
"{doc_name} <strong>$ {object.name}</strong> % i f object.origin: (بالإشارة: $ {object.origin}) % e ndif بمبلغ <strong>$ {object.amount_total} "
"$ {object.pricelist_id.currency_id.name}</strong> من $ {object.company_id.name}.</p> % i و is_online:<center style=\";text-align:right;"
"direction:rtl\"> <a href=\"${access_url}\" style=\"background-color: #1abc9c; padding: 20px; text-decoration: none; color: #fff; border-"
"radius: 5px; font-size: 16px;\" class=\"o_default_snippet_text\">{access_name} دولارًا أمريكيًا</a><br/><br/> <span style=\"color:#888888\">(أو "
"عرض ملف PDF المرفق)</span></center><br/> % e ndif<p style=\";text-align:right;direction:rtl\"> يمكنك الرد على هذا البريد الإلكتروني إذا كان "
"لديك أي أسئلة.</p><p style=\";text-align:right;direction:rtl\"> شكرا لك،</p><p j=2 style=\";text-align:right;direction:rtl\"> % i f object."
"user_id و object.user_id.signature: $ {object.user_id.signature | آمن} % e ndif</p>"

