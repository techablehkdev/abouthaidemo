"                           &nbsp;&nbsp;Order number: <strong>${object.name}</strong><br />\n"
"                           &nbsp;&nbsp;Order total: <strong>${object.amount_total} ${object.company_id.currency_id.name}</strong><br />\n"
"                           &nbsp;&nbsp;Order date: ${object.date_order}<br />\n"
"                           % if object.user_id:\n"
"                           &nbsp;&nbsp;Cashier: ${object.user_id.name}\n"
"                           % endif\n"
"                        </p>\n"
"                        <p> Product Details </p>\n"
"                        % if object.lines:\n"
"                            <table border=1>\n"
"                                <tbody border=1>\n"
"                                <tr>\n"
"                                    <th>Product Name</th>\n"
"                                    <th>Quantity</th>\n"
"                                    <th>Unit Price</th>\n"
"                                    <th>Discount %</th>\n"
"                                    <th>Total(${object.company_id.currency_id.name})</td>\n"
"                                <tr>\n"
"                                % for line in object.lines:\n"
"                                    <tr>\n"
"                                        <td>${line.product_id.name}</td>\n"
"                                        <td style=\"text-align:right\">${line.qty}</td>\n"
"                                        <td style=\"text-align:right\">${line.price_unit}</td>\n"
"                                        <td style=\"text-align:right\">${line.discount}</td>\n"
"                                        <td style=\"text-align:right\">${line.price_subtotal_incl}</td>\n"
"                                    </tr>\n"
"                                 % endfor\n"
"                                 <tr>\n"
"                                    <td colspan=\"4\"><strong>Tax</strong></td>\n"
"                                    <td style=\"text-align:right\">${object.amount_tax}</td>\n"
"                                 </tr>\n"
"                                 <tr>\n"
"                                    <td colspan=\"4\"><strong>Total Amount</strong></td>\n"
"                                    <td style=\"text-align:right\">${object.amount_total}</td>\n"
"                                 </tr>\n"
"                                 </tbody>\n"
"                            </table>\n"
"                        % endif\n"
"                        <br/>\n"
"                        <p>Thank you for choosing ${object.company_id.name or 'us'} !</p>\n"
"                        <br/>\n"
"                        <br/>\n"
"                        <div style=\"width: 375px; margin: 0px; padding: 0px; background-color: #8E0000; border-top-left-radius: 5px 5px; "
"border-top-right-radius: 5px 5px; background-repeat: repeat no-repeat;\">\n"
"                            <h3 style=\"margin: 0px; padding: 2px 14px; font-size: 12px; color: #DDD;\">\n"
"                                <strong style=\"text-transform:uppercase;\">${object.company_id.name}</strong></h3>\n"
"                        </div>\n"
"                        <div style=\"width: 347px; margin: 0px; padding: 5px 14px; line-height: 16px; background-color: #F2F2F2;\">\n"
"                            <span style=\"color: #222; margin-bottom: 5px; display: block; \">\n"
"                            % if object.company_id.street:\n"
"                                ${object.company_id.street}<br/>\n"
"                            % endif\n"
"                            % if object.company_id.street2:\n"
"                                ${object.company_id.street2}<br/>\n"
"                            % endif\n"
"                            % if object.company_id.city or object.company_id.zip:\n"
"                                ${object.company_id.zip} ${object.company_id.city}<br/>\n"
"                            % endif\n"
"                            % if object.company_id.country_id:\n"
"                                ${object.company_id.state_id and ('%s, ' % object.company_id.state_id.name) or ''} ${object.company_id."
"country_id.name or ''}<br/>\n"
"                            % endif\n"
"                            </span>\n"
"                            % if object.company_id.phone:\n"
"                                <div style=\"margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; "
"padding-right: 0px; padding-bottom: 0px; padding-left: 0px; \">\n"
"                                    Phone:&nbsp; ${object.company_id.phone}\n"
"                                </div>\n"
"                            % endif\n"
"                            % if object.company_id.website:\n"
"                                <div>\n"
"                                    Web :&nbsp;<a href=\"${object.company_id.website}\">${object.company_id.website}</a>\n"
"                                </div>\n"
"                            %endif\n"
"                            <p></p>\n"
"                        </div>\n"
"                    </div>\n"
"                "
msgstr ""
"<div j=0 style=\";text-align:right;direction:rtl\"><p style=\";text-align:right;direction:rtl\">مرحبًا $ {object.partner_id و object.partner_id."
"name أو &#39;&#39;} ،</p><p j=1 style=\";text-align:right;direction:rtl\"> <strong>المراجع</strong><br /> رقم الطلب: <strong>$ {object.name}</"
"strong><br /> إجمالي الطلب: <strong>$ {object.amount_total} $ {object.company_id.currency_id.name}</strong><br /> تاريخ الطلب: $ {object."
"date_order}<br /> % i f object.user_id: Cashier: $ {object.user_id.name} % e ndif</p><p style=\";text-align:right;direction:rtl\"> تفاصيل "
"المنتج</p> % i و object.lines:<table j=2 style=\";text-align:right;direction:rtl\"><tbody border=1><tr><th> اسم المنتج</th><th> كمية</th><th> "
"سعر الوحده</th><th> خصم ٪</th><th> الإجمالي ($ {object.company_id.currency_id.name})</td><tr> % f أو سطر في object.lines:<tr><td> {line."
"product_id.name} دولارًا أمريكيًا</td><td style=\"text-align:right\"> {line.qty} دولارًا أمريكيًا</td><td style=\"text-align:right\"> {line."
"price_unit} دولارًا أمريكيًا</td><td style=\"text-align:right\"> {line.discount} دولارًا أمريكيًا</td><td style=\"text-align:right\"> {line."
"price_subtotal_incl} دولارًا أمريكيًا</td></tr> % e ndfor<tr><td colspan=\"4\"> <strong>ضريبة</strong></td><td style=\"text-align:right\"> "
"{object.amount_tax} دولارًا أمريكيًا</td></tr><tr><td colspan=\"4\"> <strong>المبلغ الإجمالي</strong></td><td style=\"text-align:right\"> "
"{object.amount_total} دولارًا أمريكيًا</td></tr></tbody></table> % e ndif<br/><p style=\";text-align:right;direction:rtl\"> شكرًا لاختيارك $ "
"{object.company_id.name أو &#39;us&#39;}!</p><br/><br/><div j=6 style=\";text-align:right;direction:rtl\"><h3 j=7 style=\";text-align:right;"
"direction:rtl\"> <strong style=\"text-transform:uppercase;\">{object.company_id.name} دولارًا أمريكيًا</strong></h3></div><div j=9 style=\";text-"
"align:right;direction:rtl\"> <span style=\"color: #222; margin-bottom: 5px; display: block; \">% i f object.company_id.street: $ {object."
"company_id.street}<br/> % e ndif % i f object.company_id.street2: $ {object.company_id.street2}<br/> % e ndif % i f object.company_id.city or "
"object.company_id.zip: $ {object.company_id.zip} $ {object.company_id.city}<br/> % e ndif % i f object.company_id.country_id: $ {object."
"company_id.state_id و (&#39;%s ،&#39; % o bject.company_id.state_id.name) أو &#39;&#39;} $ {object.company_id. country_id.name أو &quot;&quot;}"
"<br/> % e ndif</span> % i f object.company_id.phone:<div j=11 style=\";text-align:right;direction:rtl\"> الهاتف: {object.company_id.phone} "
"دولارًا</div> % e ndif % i f object.company_id.website:<div style=\";text-align:right;direction:rtl\"> الويب: <a href=\"${object.company_id."
"website}\">{object.company_id.website} دولارًا</a></div> %e ndif<p style=\";text-align:right;direction:rtl\"></p></div></div>"

